    <rules>
        <fullName>Payment overdue alert</fullName>
        <active>false</active>
        <description>An email will be generated when an invoice for a vocational evaluation is submitted and payment is overdue.</description>
        <formula>TODAY ( )   &gt;=    Invoice_Submitted_Date__c  + 1 &amp;&amp; ( ISBLANK( Payment_Received_Date__c )) &amp;&amp;  ISPICKVAL( Benchmark_Name__c, &apos;Vocational Evaluation&apos;)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <offsetFromField>Benchmark__c.CreatedDate</offsetFromField>
            <timeLength>2</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>VE overdue payment</fullName>
        <active>false</active>
        <description>An email will be generated when an invoice for a vocational evaluation is submitted and payment is overdue.</description>
        <formula>TODAY ( )   &gt;=    Invoice_Submitted_Date__c  + 1 &amp;&amp; ( ISBLANK( Payment_Received_Date__c )) &amp;&amp;  ISPICKVAL( Benchmark_Name__c, &apos;Vocational Evaluation&apos;)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <offsetFromField>Benchmark__c.Invoice_Submitted_Date__c</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
