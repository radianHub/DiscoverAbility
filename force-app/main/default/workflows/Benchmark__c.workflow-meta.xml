    <rules>
        <fullName>Congrats letter 6%2E2015</fullName>
        <actions>
            <name>Congrats_letter</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Benchmark__c.NOA_Submitted_Date__c</field>
            <operation>greaterThan</operation>
            <value>10/10/2013</value>
        </criteriaItems>
        <criteriaItems>
            <field>Benchmark__c.Benchmark_Name__c</field>
            <operation>equals</operation>
            <value>Date of Placement</value>
        </criteriaItems>
        <description>When an NOA is submitted (after IPE is ammended) congrats letter can go out.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
